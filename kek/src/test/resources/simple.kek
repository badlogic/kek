module simple

import math

structure Point
    x:float
    y:float
end

structure Triangle
    p1: Point
    p2: Point
    p3: Point
end

structure Mesh
    vertices: Vector3[]
    indices: int[]?
end


function distance(p1: simple.Point, p2: Point): float
    var distance = (p2.x - p1.x) * (p2.x - p1.x) + (p2.y - p1.y) * (p2.y - p1.y)
    var distance = 1
    return sqrt(distance)
end

function emptyReturn ()
    return
end

function nullReturn ()
    return null
end